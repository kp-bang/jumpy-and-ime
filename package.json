{
  "name": "jumpy-and-ime",
  "displayName": "jumpy-and-ime",
  "description": "把jumpy的功能和控制系统ime中英文变化的能力聚合到一个插件上",
  "version": "0.0.1",
  "publisher": "bang",
  "engines": {
    "vscode": "^1.104.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "jumpy-and-ime.jumpy-word",
        "title": "Jumpy Word Mode"
      },
      {
        "command": "jumpy-and-ime.jumpy-exit",
        "title": "Exit Jumpy Mode"
      },
      {
        "command": "jumpy-and-ime.jumpy-up-5lines",
        "title": "viewport向上移动5行"
      },
      {
        "command": "jumpy-and-ime.jumpy-down-5lines",
        "title": "viewport向下移动5行"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "yarn run compile && yarn run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "^4.6.0",
    "@types/lodash": "^4.17.20",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@types/vscode": "^1.104.0",
    "@typescript-eslint/eslint-plugin": "^8.31.1",
    "@typescript-eslint/parser": "^8.31.1",
    "@vscode/test-cli": "^0.0.11",
    "@vscode/test-electron": "^2.5.2",
    "eslint": "^9.25.1",
    "prettier": "^3.6.2",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "eventemitter3": "^5.0.1",
    "ffi-rs": "^1.3.0",
    "lodash": "^4.17.21",
    "mobx": "^6.12.0",
    "rxjs": "^7.8.2",
    "vscode-oniguruma": "^1.6.2",
    "vscode-textmate": "^6.0.0"
  },
  "extensionDependencies": [
    "bang.hscopes-booster"
  ]
}
